  ///
  /// \snippet ScriptComponent.cpp ExecuteScriptWithResult
  HRESULT ExecuteScriptWithResult(
      [in] LPCWSTR javaScript,
      [in] ICoreWebView2ExperimentalExecuteScriptWithResultCompletedHandler* handler);
